# -*- mode: snippet; require-final-newline: nil -*-
# name: use-package
# key: use
# binding: direct-keybinding
# --
(use-package foo
  :ensure t
  :defer t
  :bind (("C-c y" . hoge))
  :init
  (progn
    (setq foo-variable t)
    (foo-mode 1)
    )
  :config
  (progn
    (setq foo-variable t)
    (foo-mode 1))
    )